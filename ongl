echo "Start"
mount -o rw,remount /
mount -o rw,remount /system
curl -o /etc/hosts https://raw.githubusercontent.com/abo7amzehh/myhost/master/Ghosts
wget -O /etc/hosts https://raw.githubusercontent.com/abo7amzehh/myhost/master/Ghosts
echo ""
#
rm -rf /storage/emulated/0/Tencent
rm -rf /storage/emulated/0/.backups
#
chmod -R 554 /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Config/Android/AntiCheat.ini
chmod -R 554 /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Config/Android/*
chmod -R 554 /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Config/Android/*/*
#
chmod -R 000 /storage/emulated/0/Tencent
chmod -R 000 /storage/emulated/0/Tencent/*
chmod -R 000 /storage/emulated/0/Tencent/*/*
chmod -R 000 /storage/emulated/0/Tencent/*/*/*
chmod -R 000 /storage/emulated/0/Tencent/*/*/*/*
am start -a android.intent.action.MAIN -n com.gnkzqdqkufhbz/.ActivityMain
sleep 2
am start -n com.tencent.ig/com.epicgames.ue4.SplashActivity
sleep 5
chmod -R 554 /data/data/com.tencent.ig/lib/libtersafe.so
chmod -R 600 /data/data/com.tencent.ig/databases/*
sleep 10
#
bypass()
{
rm -rf /data/data/com.tencent.ig/app_appcache
rm -rf /data/data/com.tencent.ig/app_bugly
rm -rf /data/data/com.tencent.ig/cache
rm -rf /data/data/com.tencent.ig/code_cache
printf "\n Protection On \n";
}
PACKAGE='com.tencent.ig'
while [ $(pidof $PACKAGE) ]
do
bypass
((COUNT=$COUNT+10))
if [ ! $(pidof $PACKAGE) ]; then
break
fi
done
echo "finish"
